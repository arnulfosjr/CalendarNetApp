{
  "expo": {
    "name": "CalendarNet",
    "slug": "CalendarNet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/src/assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/src/assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2960AC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "permission": ["network","alert","badge","sound"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/src/assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/src/assets/images/logo.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-router"
    ]
  }
}
